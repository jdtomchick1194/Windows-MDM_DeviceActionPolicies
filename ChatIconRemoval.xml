<!--This will remove the Chat icon from the taskbar on Windows 11. Required when wanting to completely remove MS Teams-->
<!--These are the install settings-->
<Replace>
      <CmdID>a8cf6e44-276d-4680-97a5-f6541f16fba7</CmdID>
      <Item>
        <Target>
          <LocURI>
            ./Vendor/MSFT/Policy/Config/Experience/ConfigureChatIcon
          </LocURI>
        </Target>
        <Meta>
          <Format xmlns="syncml:metinf">int</Format>
          <Type>text/plain</Type>
        </Meta>
        <Data>3</Data>
      </Item>
    </Replace>
<!--These are the remove settings-->
<Delete>
      <CmdID>a8cf6e44-276d-4680-97a5-f6541f16fba7</CmdID>
      <Item>
        <Target>
          <LocURI>
            ./Vendor/MSFT/Policy/Config/Experience/ConfigureChatIcon
          </LocURI>
        </Target>
        <Meta>
          <Format xmlns="syncml:metinf">int</Format>
          <Type>text/plain</Type>
        </Meta>
        <Data>3</Data>
      </Item>
    </Delete>
